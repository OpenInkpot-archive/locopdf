#Refer to en.po for texts
msgid "locopdf1"
msgstr ""
"<h2>Commandes:</h2>\n"
"<p><b>0</b> - montrer fichier d'aide</p>\n"
"<p><b>1</b> - montrer la page</p>\n"
"<p><b>2</b> - table des matières</p>\n"
"<p><b>4</b> - réinitialiser l'aggrandissement à 100%</p>\n"
"<p><b>5</b> - et <b>Rechercher/Zoom</b> - échelle (en pourcentage)</p>\n"
"<p><b>6</b> - agrandir (utilisant l'incrément specifié dans les préférences)</p>\n"
"<p><b>7</b> - réduire </p>\n"
"<p><b>8</b> - montrer la page précédente </p>\n"
"<p><b>9</b> - montrer la page suivante</p>\n"
"<p><b>Rotation</b> - changer l'orientation de la page</p>\n"
"<p><b>Haut/Bas</b> et roue de contrôle - page suivante/précedente</p>\n"
"<p><b>Menu</b> - retourner vers menu principal</p>\n"
"<p><b>C</b> - retourner vers menu précédent</p>\n"
"<p><b>OK</b> - préférences</p>"

